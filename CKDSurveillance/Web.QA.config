<?xml version="1.0" encoding="utf-8"?>

<!-- For more information on using web.config transformation visit http://go.microsoft.com/fwlink/?LinkId=125889 -->

<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <appSettings>
    <add key="DBServer" value="2016Q1" xdt:Transform="SetAttributes" xdt:Locator="Match(key)" />
    <add key="Database" value="NCCD_DDT_CKD" xdt:Transform="SetAttributes" xdt:Locator="Match(key)" />
    <add key="MailServer" value="smtpgw.cdc.gov" xdt:Transform="SetAttributes" xdt:Locator="Match(key)" />
    <add key="MailSender" value="DONOTREPLY_DDT_CKD@cdc.gov" xdt:Transform="SetAttributes" xdt:Locator="Match(key)" />
    <add key="SupportMailReciever" value="wrs3@cdc.gov" xdt:Transform="SetAttributes" xdt:Locator="Match(key)" />
    <add key="ExceptionMailReciever" value="nccdphpapplicationex@cdc.gov" xdt:Transform="SetAttributes" xdt:Locator="Match(key)" />
    <add key="ExceptionMailSubject" value="DDT CKD - QA - Application Exception Token:"  xdt:Transform="SetAttributes" xdt:Locator="Match(key)" />
    <add key="EmailExceptions" value="False" xdt:Transform="SetAttributes" xdt:Locator="Match(key)" />
    <add key="ErrorLogType" value="ALL" xdt:Transform="SetAttributes" xdt:Locator="Match(key)" />
    <add key="ErrorLogAPI" value="https://nccdgisdev.cdc.gov/Logger/ApplicationError" xdt:Transform="SetAttributes" xdt:Locator="Match(key)" />
    
  </appSettings>
  <system.web>
    <compilation xdt:Transform="RemoveAttributes(debug)" />
    <httpCookies httpOnlyCookies="true" requireSSL="true" xdt:Transform="Replace"/>
  </system.web>
  <connectionStrings xdt:Transform="Remove" >
  </connectionStrings>
</configuration>